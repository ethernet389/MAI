<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="QButtonStyle" parent="@style/Widget.AppCompat.Button">
        <item name="qb_radius">25</item>
        <item name="qb_backgroundColor">#D8E0DBDB</item>
        <item name="qb_strokeColor">#565050</item>
        <item name="qb_strokeWidth">5</item>
        <item name="android:layout_margin">5dp</item>
    </style>

    <style name="CustomEditTextStyle" parent="@style/Theme.AppCompat.Dialog">
        <item name="libIZO_setBorderColor">#565050</item>
        <item name="libIZO_setClearIconVisible">false</item>
        <item name="libIZO_setCornerRadius">10dp</item>
        <item name="libIZO_setBorderView">true</item>
        <item name="libIZO_setStrokeWidth">2dp</item>
        <item name="libIZO_setBackgroundColor">#4CAB11</item>
    </style>

    <style name="ButtonStyle" parent="@style/Widget.AppCompat.Button">
        <item name="background">@drawable/button_area_shape</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:gravity">center</item>
        <item name="textAllCaps">false</item>
    </style>
</resources>